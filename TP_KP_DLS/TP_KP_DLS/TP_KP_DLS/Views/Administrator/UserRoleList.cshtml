@model IEnumerable<TP_KP_DLS.Models.UserRole>

@{
    ViewBag.Title = "UserRoleList";
}

<h2>UserRoleList</h2>

<p>
    @Html.ActionLink("Create New", "CreateUserRole")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RoleId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RoleId)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditUserRole", new {  id=item.UserId  }) |
            @Html.ActionLink("Details", "DetailsUserRole", new {  id=item.UserId }) |
            @Html.ActionLink("Delete", "DeleteUserRole", new {  id=item.UserId })
        </td>
    </tr>
}

</table>
